/* istanbul ignore next */'use strict';it('shows forks when the button is tapped', function () {
    var rendered = TestUtils.renderIntoDocument(
    /* istanbul ignore next */React.createElement(Detail, /* istanbul ignore next */{ params: { repo: '' } }));


    var btns = TestUtils.scryRenderedDOMComponentsWithTag(rendered, 'button');
    var forksButton = btns[1];
    TestUtils.Simulate.click(forksButton);
    expect(rendered.state.mode).toEqual('forks');});